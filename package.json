{
	"name": "webvalidator",
	"displayName": "Web Validator",
	"description": "Check the validity of an HTML file with the W3C Validator API",
	"publisher": "CelianRiboulet",
	"repository": "https://github.com/celian-rib/vscode-webvalidation-extension",
	"version": "1.0.2",
	"icon": "images/web-validator-icon.png",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:webvalidator.startvalidation"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "webvalidator.startvalidation",
				"title": "Start Web validation"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"axios": "^0.21.1"
	}
}
